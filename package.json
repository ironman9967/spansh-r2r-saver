{
	"name": "spansh-r2r-saver",
	"version": "0.0.1",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build-client-env": "rm -rf dist/public && webpack --config webpack.config.client.js --mode",
		"build-client-dev": "npm run build-client-env development",
		"build-client": "npm run build-client-env production",
		"build-service": "rm -rf dist && babel src/service -d dist",
		"build-env": "npm run build-service && npm run build-client-env",
		"build-dev": "npm run build-env development",
		"build": "npm run build-env production",
		"start-service": "node dist/index.js",
		"start-service-only": "npm run build-service && npm run start-service",
		"start-dev": "npm run build-dev && npm run start-service",
		"start": "npm run build && npm run start-server"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"form-data": "^2.3.3",
		"hapi": "^18.1.0",
		"inert": "^5.1.2",
		"lodash": "^4.17.11",
		"node-fetch": "^2.3.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"html-webpack-plugin": "^3.2.0",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.2.3"
	},
	"babel": {
		"presets": [
			"@babel/preset-env"
		],
		"plugins": [
			"@babel/plugin-proposal-object-rest-spread"
		]
	}
}
